# Anagram sets
First we need to sort by the first field of the signed words.
<<:make>>=
en-signed-sorted: en-signed
	sort --key=1 --stable $^ > $@

en-sorted-signatures: en-signed-sorted
	cut --fields=1 $^ > $@

en-set-lengths: en-sorted-signatures
	uniq --count $^ | cut --fields=1 > $@

en-set-length-counts: en-set-lengths
	uniq --count $@
